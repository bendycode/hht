<h1>Listing venues</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Lat/Lng</th>
    <th>Type</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Phone</th>
  </tr>

<% @venues.each do |venue| %>
  <tr>
    <td><%=h venue.name %></td>
    <td><%= venue.lat %>/<%=venue.lng%></td>
    <td><%=h venue.type %></td>
    <td><%=h venue.address1 %></td>
    <td><%=h venue.address2 %></td>
    <td><%=h venue.city %></td>
    <td><%=h venue.state %></td>
    <td><%=h venue.zip %></td>
    <td><%=h venue.phone %></td>
    <td><%= link_to 'Show', venue %></td>
    <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
    <td><%= link_to 'Destroy', venue, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New venue', new_venue_path %>